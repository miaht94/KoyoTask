{
    "appendNewTask" : {
        "targetId" : "dashboard-tasks",
        "html" : "<div class='d-flex flex-row row task-frame' id='dashboarditem'><div class='d-flex col-1 justify-content-start' id='dashboarditemcol'><label class='container_checkbox'><input type='checkbox' name='radio' id='taskCheckbox'><span class='checkmark'></span></label></div><div class='d-flex flex-column col-9 task-data'><div class='d-flex col-11 task-title' id = 'taskTitleForEdit' contenteditable='true'> {{task_name}}</div><div class='d-flex col-1'></div><div class='d-flex col-12 task-time'>{{task_date}}</div><div class='d-flex col-11 task-description'>{{task_description}}</div><div class='d-flex col-1'></div></div></div></div>" 
    },

    "TaskButtonGroup" :{
        "targetId" : "dashboard-tasks",
        "html" : "<div class='d-flex flex-column col-2 task-data' id='taskButtonGroup' style='opacity:0'><button type='button' class='btn btn-secondary btn-danger' id = 'taskDeleteButton'>Delete</button><button type='button' class='btn btn-secondary' id = 'taskExpandButton'>Expand</button></div>"
    },

    "TaskAdd" :{
        "targetId" : "dashboarditem",
        "html" : "<div class='d-flex flex-row row task-frame' id='dashboarditem'><div class='d-flex col-1 justify-content-start' id='dashboarditemcol'><label class='container_checkbox'><input type='checkbox' name='radio' id = checkboxForAdd><span class='checkmark'></span></label></div><div class='d-flex flex-column col-9 task-data'><div class='d-flex col-11 task-title' id = 'taskTitleForAdd' contenteditable='true' placeholder='Enter new task title'></div></div></div></div>" 
    },

    "appendNewList" : {
        "targetId" : "list-group",
        "html" : "<div class='d-flex flex-row p-1 list-group-item align-items-center'id='listitem'><div class='d-flex col-2 justify-content-center'id='list_icon'><i class='bi {{list_icon}} list-icon'></i></div><div class='d-flex flex-column col-10 p-1 justify-content-start align-content-center noselect'id='listtite'><div class='d-flex flex-row p-0 list-group-title'id='list_name'>{{list_name}}</div><div class='d-flex flex-row p-0 list-group-notice'id='list_description'>{{list_description}}</div></div></div>"
    },

    "displayChosenList" : {
        
    },
    "userInfo" : {
        "html" : "<div class='col col-4 col-sm-4 align-items-center'><div class='d-flex p-0 justify-content-center'id='UserAvt'><img src={{avtURL}}class='user-avatar rounded-circle'></img></div></div><div class='col col-8 col-sm-8 d-flex align-items-center'><div class='d-flex flex-column'><div class='username p-0'id='Username'>{{username}}</div><div class='usermail p-0'id='UserEmail'>{{useremail}}</div></div></div>"
    }
